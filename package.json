{
  "name": "hello",
  "description": "a wordpress theme for our landing site",
  "version": "0.1.0",
  "homepage": "https://github.com/democracy-os-climate/hello#readme",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/democracy-os-climate/hello.git"
  },
  "author": "moustachu@democracyos.eu",
  "license": "MIT",
  "keywords": [
    "yo",
    "yeoman",
    "gulp-webapp",
    "wordpress",
    "sass",
    "jade"
  ],
  "main": "gulpfile.babel.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "bugs": {
    "url": "https://github.com/democracy-os-climate/hello/issues"
  },
  "engines": {
    "node": "4.0.0",
    "npm": "2.14.2"
  },
  "devDependencies": {
    "babel-core": "^5.5.6",
    "browser-sync": "^2.2.1",
    "chalk": "^1.1.1",
    "del": "^1.1.1",
    "eslint": "^1.8.0",
    "eslint-config-airbnb": "^0.1.0",
    "git-branch": "^0.3.0",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-cache": "^0.2.8",
    "gulp-eslint": "^1.0.0",
    "gulp-if": "^1.2.5",
    "gulp-imagemin": "^2.2.1",
    "gulp-jade": "^1.1.0",
    "gulp-load-plugins": "^0.10.0",
    "gulp-minify-css": "^1.1.1",
    "gulp-minify-html": "^1.0.0",
    "gulp-plumber": "^1.0.1",
    "gulp-rename": "^1.2.2",
    "gulp-sass": "^2.0.0",
    "gulp-size": "^1.2.1",
    "gulp-sourcemaps": "^1.5.0",
    "gulp-uglify": "^1.1.0",
    "gulp-useref": "^1.1.1",
    "gulp-util": "^3.0.7",
    "imagemin-pngcrush": "^4.1.0",
    "jade": "^1.11.0",
    "main-bower-files": "^2.5.0",
    "opn": "^1.0.1",
    "phpjade": "^1.4.1",
    "vinyl-ftp": "^0.4.5",
    "wiredep": "^2.2.2"
  },
  "eslintConfig": {
    "extends": "airbnb/legacy",
    "env": {
      "node": true,
      "browser": true,
      "jquery": true
    },
    "rules": {
      "quotes": [
        2,
        "single"
      ],
      "strict": [
        2,
        "global"
      ],
      "comma-dangle": [
        1,
        "always-multiline"
      ],
      "no-console": 0,
      "func-names": 0,
      "id-length": [
        2,
        {
          "exceptions": [
            "$",
            "e",
            "i",
            "k",
            "v",
            "o",
            "t"
          ]
        }
      ]
    }
  },
  "dependencies": {
    "imagemin-pngcrush": "^4.1.0"
  }
}
